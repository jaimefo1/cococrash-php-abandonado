program COCO_CRASH;

   uses crt, U_MENU, U_GESMOD, U_CUBO;

var
     OP : char;

begin
     repeat
           CLRSCR;
           MENU ('MENU PRINCIPAL','#1MODULOS #2CUBO #0SALIR',OP, '');
           case OP of
                '1' : GES_MODULOS;
                '2' : SOL_CUBO
           end
     until OP = '0'
end.
